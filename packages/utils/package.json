  "scripts": {
    "command:example": "pnpm <command> --filter @odinlin/utils",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c"
  },
  "dependencies": {
    "await-to-js": "^3.0.0"
