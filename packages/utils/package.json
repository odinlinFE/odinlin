{
  "name": "@odinlin/utils",
  "version": "0.0.1",
  "description": "Opinionated collection of common JavaScript / TypeScript utils",
  "author": {
    "name": "Lin Hao",
    "email": "topazur@163.com",
    "url": "https://github.com/odinlin"
  },
  "license": "MIT",
  "homepage": "https://github.com/odinlin/odinlin/tree/main/packages/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odinlin/odinlin.git",
    "directory": "packages/utils"
  },
  "bugs": {
    "url": "https://github.com/odinlin/odinlin/issues"
  },
  "keywords": [
