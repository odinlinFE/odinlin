{
  // [Why I don't use Prettier](https://antfu.me/posts/why-not-prettier-zh)
  "prettier.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  // cSpell list
  "cSpell.words": [
    "dumi",
    "esbuild",
    "iife",
    "jsdelivr",
    "mfsu",
    "NOINLINE",
    "npmrc"
  ]
}
